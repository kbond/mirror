{
    "name": "zenstruck/mirror",
    "description": "Alternate Reflection API and related helpers.",
    "homepage": "https://github.com/zenstruck/mirror",
    "type": "library",
    "license": "MIT",
    "keywords": ["reflection"],
    "authors": [
        {
            "name": "Kevin Bond",
            "email": "kevinbond@gmail.com"
        }
    ],
    "require": {
        "php": ">=8.0",
        "symfony/polyfill-php81": "^1.27"
    },
    "require-dev": {
        "doctrine/collections": "^1.8",
        "phpstan/phpstan": "^1.4",
        "phpunit/phpunit": "^9.5.0",
        "symfony/phpunit-bridge": "^6.1",
        "symfony/property-access": "^5.4|^6.0",
        "symfony/var-dumper": "^5.4|^6.0"
    },
    "suggest": {
        "symfony/property-access": "To use the PropertyAccessHydrator"
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true
    },
    "autoload": {
        "psr-4": { "Zenstruck\\": ["src/"] }
    },
    "autoload-dev": {
        "psr-4": { "Zenstruck\\Tests\\": ["tests/"] }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
